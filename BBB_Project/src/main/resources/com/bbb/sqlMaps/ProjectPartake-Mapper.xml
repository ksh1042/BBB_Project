<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ProjectPartake">
	
	<select id="selectProjectPartakeList" resultType="int">
   		select pjnum
   		from tbl_project_partake
   		where id=#{id}
	</select>
	
	<select id="selectBindingProject" resultType="projectPartake">
		select *
		from tbl_project_partake
		where id=#{id}
	</select>
	
</mapper>
