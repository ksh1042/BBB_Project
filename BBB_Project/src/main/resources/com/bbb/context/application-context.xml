<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="memberDAO" class="com.bbb.dao.MemberDAOImpl"
		p:sqlSession-ref="sqlSession" />

	<bean id="memberService" class="com.bbb.service.MemberServiceImpl"
		p:memberDAO-ref="memberDAO" />
	
	<!-- 이병수  프로젝트 리스트 페이지-->	
	<bean id="projectPartakeDAO" 
		  class="com.bbb.dao.ProjectPartakeDAOImpl"
		  p:sqlSession-ref="sqlSession"
		  />
	
	<bean id="projectPartakeService" 
		  class="com.bbb.service.ProjectPartakeServiceImpl"
		  p:projectPartakeDAO-ref="projectPartakeDAO"
		  />
		  
	<!-- Project -->
	<bean id="projectDAO"
		  class="com.bbb.dao.ProjectDAOImpl"
		  p:sqlSession-ref="sqlSession"/>
	<bean id="projectService"
		  class="com.bbb.service.ProjectServiceImpl"
		  p:projectDAO-ref="projectDAO"/>
	
</beans>
