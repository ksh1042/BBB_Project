<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="memberDAO" class="com.bbb.dao.MemberDAOImpl"
		p:sqlSession-ref="sqlSession" />

	<bean id="memberService" class="com.bbb.service.MemberServiceImpl"
		p:memberDAO-ref="memberDAO" />
	<bean id="boardDAO"
		class="com.bbb.dao.BoardDAOImpl"
		p:session-ref="sqlSession"
	/>
	<bean id="boardService"
		class="com.bbb.service.BoardServiceImpl"
		p:boardDAO-ref="boardDAO" 
	/>
	<bean id="noticeDAO"
		class="com.bbb.dao.NoticeDAOImpl"
		p:session-ref="sqlSession"
	/>
	<bean id="noticeService"
		class="com.bbb.service.NoticeServiceImpl"
		p:noticeDAO-ref="noticeDAO"
	/>
	
</beans>
