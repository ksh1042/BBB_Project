<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="memberDAO" class="com.bbb.dao.MemberDAOImpl"
		p:sqlSession-ref="sqlSession" />

	<bean id="memberService" class="com.bbb.service.MemberServiceImpl"
		p:memberDAO-ref="memberDAO" />
	
	<!-- 이병수  프로젝트 리스트 페이지-->	
	
	<bean id="projectDAO" 
		  class="com.bbb.dao.ProjectDAOImpl"
		  p:sqlSession-ref="sqlSession"
		  />
		  
	<bean id="projectPartakeDAO" 
		  class="com.bbb.dao.ProjectPartakeDAOImpl"
		  p:sqlSession-ref="sqlSession"
		  />
	
	<bean id="projectService" 
		  class="com.bbb.service.ProjectServiceImpl"
		  p:projectDAO-ref="projectDAO"
		  p:projectPartakeDAO-ref="projectPartakeDAO"
		  />
	<bean id="qnaDAO"
	      class="com.bbb.dao.QnaDAOImpl"
	      p:session-ref="sqlSession"
	      />
	<bean id="qnaService"
		  class="com.bbb.service.QnaServiceImpl"
		  p:qnaDAO-ref="qnaDAO"
		  />
	<bean id="qnaReplyDAO"
	      class="com.bbb.dao.QnaReplyDAOImpl"
	      p:session-ref="sqlSession" 
	      />
	<bean id="qnaReplyService"
		  class="com.bbb.service.QnaReplyServiceImpl"
		  p:qnaReplyDAO-ref="qnaReplyDAO"
		  />
	<bean id="financeDAO"
		  class="com.bbb.dao.FinanceDAOImpl"
		  p:session-ref="sqlSession"
		  />
	<bean id="financeDetailDAO"
	 	  class="com.bbb.dao.FinanceDetailDAOImpl"
	 	  p:session-ref="sqlSession"
	 	  />
	<bean id="financeService"
		  class="com.bbb.service.FinanceServiceImpl"
		  p:financeDAO-ref="financeDAO"
		  p:financeDetailDAO-ref="financeDetailDAO"
		  />
	
	
</beans>












